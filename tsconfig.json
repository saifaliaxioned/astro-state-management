{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@store": [
        "src/store"
      ],
      "@utils": [
        "src/utils"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}